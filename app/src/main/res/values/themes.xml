<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.ProminentColors" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/ruby</item>
        <item name="colorPrimaryVariant">@color/ruby_var</item>
        <item name="colorOnPrimary">@color/creme_white</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/discord</item>
        <item name="colorSecondaryVariant">@color/discord_var</item>
        <item name="colorOnSecondary">@color/independence_dark</item>

        <!-- Third color -->
        <item name="colorTertiary">@color/winter_dream</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorOnSecondary</item>

        <!-- Customize your theme here. -->
        <item name="android:textColor">@color/black</item>
    </style>
</resources>